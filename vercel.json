{
  "version": 2,
  "functions": {
    "api/analyze.py": {
      "runtime": "@vercel/python"
    }
  },
  "routes": [
    { "src": "/api/analyze", "dest": "/api/analyze.py" },
    { "src": "/api/status", "dest": "/api/status.js" },
    { "src": "/", "dest": "/public/index.html" },
    { "src": "/(.*)", "dest": "/public/$1" }
  ]
}